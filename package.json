  "scripts": {
    "heroku-postbuild": "npm run build:prod",
    "start": "node server",
    "dev": "concurrently --kill-others --raw \"npm run build:dev:watch\" \"npm run serve\"",
    "clean": "rimraf public",
    "lint:ts": "tslint --format stylish -p tsconfig.json --type-check '*.ts' 'app/**/*.ts' 'config/**/*.ts' 'server/**/*.ts'",
    "lint:js": "eslint '*.js' 'app/**/*.js' 'config/**/*.js' 'server/**/*.js'",
    "lint:ts:ci": "tslint --format stylish -c tslint.ci.json '*.ts' 'app/**/*.ts' 'config/**/*.ts' 'server/**/*.ts'",
    "lint:js:ci": "eslint -c .eslintrc-ci '*.js' 'app/**/*.js' 'config/**/*.js' 'server/**/*.js'",
    "lint:sass": "sass-lint '**/*.scss' -v --no-exit -f stylish -i 'app/assets/styles/lib/**/*, node_modules/**/*'",
    "lint": "npm run lint:ts && npm run lint:js && npm run lint:sass",
    "build:prod": "rimraf public && webpack --config config/webpack.prod.js --progress --color --profile --display-modules --display-chunks --bail",
    "build:dev": "rimraf public && webpack --config webpack.config.js --progress --color --profile --bail",
    "build:dev:watch": "rimraf public && webpack --config webpack.config.js --progress --color --profile --watch",
    "build": "npm run build:dev:watch",
    "serve": "nodemon -V -w server/ server.js",
    "test": "rimraf coverage && karma start",
    "test:watch": "rimraf coverage && karma start --auto-watch --no-single-run",
    "test:debug": "rimraf coverage && karma start --auto-watch --no-single-run --browsers=Chrome",
    "fontgen": "fontmin app/assets/fontsrc app/assets/fonts && rm app/assets/fonts/*.css"
  },
