div.compass-modal
  h1.compass-modal-title {{ modalInputs.title }}
  div.compass-modal-body {{ modalInputs.body }}
  div.compass-modal-action-bar
    span.compass-modal-btn((click)='hideModal("cancel")') {{ modalInputs.rejectLabel }}
    button.btn-action((click)='hideModal("archive")') {{ modalInputs.acceptLabel }}
