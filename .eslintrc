{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jquery": true,
    "jasmine": true
  },
  "extends": "standard",
  "plugins": [
    "standard"
  ],
  "globals": {
    "angular": false,
    "module": false,
    "inject": false
  },
  "rules": {
    "indent": 0,
    // "no-console": 1,
    "no-multi-spaces": 0,
    "no-debugger": 0,
    "no-path-concat": 0,
    "no-unused-vars": 1,
    "one-var": [0, "always"],
    "padded-blocks": 0,
    "quotes": [1, "single"],
    "semi": [2, "always"],
    "space-before-function-paren": 0,
    "eqeqeq": 1
  }
}
